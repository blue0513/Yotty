{
  "name": "Yotty",
  "version": "1.0",
  "description": "Quick access to your Google Calendar via Google API",
  "manifest_version": 3,
  "action": {
    "default_title": "Yotty",
    "default_popup": "index.html"
  },
  "key": "YOUR_PUBLIC_KEY",
  "oauth2": {
    "client_id": "YOUR_CLIENT_ID",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "mac": "Alt+C"
      }
    }
  },
  "icons": {
    "16": "favicon/favicon16.png",
    "32": "favicon/favicon32.png",
    "48": "favicon/favicon48.png",
    "128": "favicon/favicon128.png"
  }
}
